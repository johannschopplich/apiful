{
  "name": "apiverse",
  "type": "module",
  "version": "0.5.0",
  "packageManager": "pnpm@9.4.0",
  "description": "Unified, pluggable API client management",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/apiverse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/apiverse.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/apiverse/issues"
  },
  "keywords": [
    "api",
    "client",
    "nitro",
    "ofetch",
    "routes",
    "worker"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./adapters": {
      "types": "./dist/adapters.d.mts",
      "import": "./dist/adapters.mjs"
    },
    "./openapi": {
      "types": "./dist/openapi.d.mts",
      "import": "./dist/openapi.mjs"
    },
    "./schema": {
      "types": "./schema.d.mts",
      "import": "./schema.mjs"
    }
  },
  "files": [
    "dist",
    "schema.*"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "tsx playground/clients.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp",
    "prepack": "pnpm run build",
    "prepare": "tsx scripts/prepare.ts"
  },
  "dependencies": {
    "ofetch": "^1.3.4",
    "openapi-typescript": "^7.0.1",
    "openapi-typescript-helpers": "^0.0.9",
    "scule": "^1.3.0",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "consola": "^3.2.3",
    "eslint": "^9.6.0",
    "get-port-please": "^3.1.2",
    "h3": "^1.12.0",
    "listhen": "^1.7.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "unbuild": "^3.0.0-rc.5",
    "vitepress": "^1.2.3",
    "vitest": "^1.6.0"
  }
}
