{
  "name": "apiverse",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@8.15.4",
  "description": "Unified, pluggable API client management",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/apiverse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/apiverse.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/apiverse/issues"
  },
  "keywords": [
    "api",
    "client",
    "nitro",
    "ofetch",
    "routes",
    "worker"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "play": "tsx playground/clients.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepack": "pnpm run build",
    "prepare": "tsx scripts/prepare.ts"
  },
  "dependencies": {
    "ofetch": "^1.3.3",
    "scule": "^1.3.0",
    "ufo": "^1.4.0"
  },
  "optionalDependencies": {
    "openapi-typescript": "^6.7.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.1",
    "@types/node": "^20.11.26",
    "bumpp": "^9.4.0",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "get-port-please": "^3.1.2",
    "h3": "^1.11.1",
    "jiti": "^1.21.0",
    "listhen": "^1.7.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "unbuild": "^3.0.0-rc.1",
    "vitepress": "^1.0.0-rc.45",
    "vitest": "^1.3.1"
  }
}
